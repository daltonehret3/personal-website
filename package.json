{
  "name": "personal-website",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.14.0"
  },
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "docker:start": "docker-compose up -d --build && npm run docker:health",
    "docker:stop": "docker-compose down",
    "docker:health": "node --experimental-modules --es-module-specifier-resolution=node ./scripts/healthcheck.js",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.jsx,.tx,.tsx",
    "start": "react-scripts start",
    "test": "npm run lint && npm run coverage",
    "unit": "jest --env=jsdom",
    "verify": "npm run test && npm run docker:start && npm run docker:health && npm run cypress:run"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-loader-spinner": "5.1.4",
    "react-router-dom": "6.2.2",
    "react-scripts": "5.0.0",
    "styled-components": "5.3.3",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-syntax-jsx": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "babel-jest": "27.5.1",
    "babel-polyfill": "6.26.0",
    "chance": "1.1.8",
    "cypress": "9.5.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "frisby": "2.1.3",
    "prettier": "2.6.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
